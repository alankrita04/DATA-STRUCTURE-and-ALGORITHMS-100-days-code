//Problem: Read a string and check if it is a palindrome using two-pointer comparison.

#include <stdio.h>
#include <string.h>

int main()
{
    char s[100];
    int i, j, len, isPal = 1;

    // Read string
    scanf("%s", s);

    len = strlen(s);

    // Two pointer comparison
    i = 0;
    j = len - 1;

    while(i < j)
    {
        if(s[i] != s[j])
        {
            isPal = 0;
            break;
        }
        i++;
        j--;
    }

    if(isPal == 1)
        printf("YES");
    else
        printf("NO");

    return 0;
}
